    "@libp2p/webrtc-star": "latest",
    "@libp2p/websockets": "latest",
    "@testing-library/dom": "latest",
    "@tiptap/core": "latest",
    "bittorrent-dht": "latest",
    "cors": "latest",
    "express": "latest",
    "ipfs-core": "latest",
    "jquery": "latest",
    "libp2p": "latest",
    "socket.io": "latest",
    "uuid": "latest",
    "vite": "latest",
