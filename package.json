{
  "name": "mb2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/server.ts",
    "signal": "PORT=4444 node ./node_modules/y-webrtc/bin/server.js",
    "dump": "git ls-files | xargs -I{} sh -c 'echo -e \"===== {} =====\\n\"; cat {}; echo -e \"\\n\"'",
    "dumpclip": "npm run dump | xclip -i",
    "test": "vitest --run",
    "test:server": "npx vitest --run --config vitest.server.config.ts --dangerouslyIgnoreUnhandledErrors",
    "test:ui": "npx vitest --run --config vitest.ui.config.ts --printConsoleTrace --dangerouslyIgnoreUnhandledErrors",
    "test:all": "npm run test:server && npm run test:browser",
    "test:coverage": "vitest run --coverage"
  },
