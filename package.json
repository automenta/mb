{
  "name": "mb2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node server/server.ts",
    "signal": "PORT=4444 node ./node_modules/y-webrtc/bin/server.js",
    "dump": "git ls-files | xargs -I{} sh -c 'echo -e \"===== {} =====\\n\"; cat {}; echo -e \"\\n\"'",
    "dumpclip": "npm run dump | xclip -i",
    "test": "vitest --run",
    "test:server": "vitest --run --config vitest.config.js",
    "test:browser": "vitest --run --config vitest.browser.config.ts",
    "test:all": "npm run test:server && npm run test:browser",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chainsafe/libp2p-gossipsub": "latest",
